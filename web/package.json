    "@humanwhocodes/retry": "^0.4.3",
    "dlv": "^1.1.3",
    "micromatch": "^4.0.8",
    "next": "^14.2.23",
    "next-auth": "^4.24.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
